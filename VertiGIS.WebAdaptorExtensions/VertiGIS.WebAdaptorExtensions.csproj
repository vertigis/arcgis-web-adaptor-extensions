  <ItemGroup>
    <Compile Include="..\_Shared\AssemblyInfo.cs" Link="AssemblyInfo.cs" />
  </ItemGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup>
	
</Project>
